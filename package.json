{
  "name": "pr-status",
  "version": "1.0.0",
  "type": "module",
  "description": "A beautiful CLI tool to check the status of your open GitHub pull requests, categorized by priority and action needed",
  "main": "dist/index.js",
  "bin": {
    "pr-status": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "github",
    "pull-request",
    "pr",
    "status",
    "cli",
    "developer-tools",
    "code-review",
    "git",
    "github-cli"
