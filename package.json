  "author": "Rob Ennals <rob.ennals@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/robennals/pr-status"
  },
  "bugs": {
    "url": "https://github.com/robennals/pr-status/issues"
  },
  "homepage": "https://github.com/robennals/pr-status#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
